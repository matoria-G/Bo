package com.bo.bean;

public class ExceptionBean {
	
	private String entity;
	private String exceptionCode;
	private String  exceptionSrc;// -- LTADDR
	private String srcRefNum;//    --CFS Number
	private String srcTable;//	 --SGINQLOG	
	private String srcKey;//		 --pk
	private String  remarks;//		 --Error Description
	private String action;
	
	public String getAction() {
		return action;
	}
	public void setAction(String action) {
		this.action = action;
	}
	public String getEntity() {
		return entity;
	}
	public void setEntity(String entity) {
		this.entity = entity;
	}
	public String getExceptionCode() {
		return exceptionCode;
	}
	public void setExceptionCode(String exceptionCode) {
		this.exceptionCode = exceptionCode;
	}
	public String getExceptionSrc() {
		return exceptionSrc;
	}
	public void setExceptionSrc(String exceptionSrc) {
		this.exceptionSrc = exceptionSrc;
	}
	public String getSrcRefNum() {
		return srcRefNum;
	}
	public void setSrcRefNum(String srcRefNum) {
		this.srcRefNum = srcRefNum;
	}
	public String getSrcTable() {
		return srcTable;
	}
	public void setSrcTable(String srcTable) {
		this.srcTable = srcTable;
	}
	public String getSrcKey() {
		return srcKey;
	}
	public void setSrcKey(String srcKey) {
		this.srcKey = srcKey;
	}
	public String getRemarks() {
		return remarks;
	}
	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}	
}
